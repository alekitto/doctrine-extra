{
    "name": "fazland/doctrine-extra",
    "description": "A set of Doctrine extensions.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Massimiliano Braglia",
            "email": "massimiliano.braglia@gmail.com"
        },
        {
            "name": "Alessandro Chitolina",
            "email": "alekitto@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "alcaeus/mongo-php-adapter": "^1.1",
        "cakephp/chronos": "^1.2",
        "doctrine/mongodb-odm": "^1.3",
        "doctrine/phpcr-odm": "^1.5",
        "doctrine/orm": "^2.7",
        "refugis/elastica-odm": "^1.0@dev",
        "friendsofphp/php-cs-fixer": "^2.13",
        "giggsey/libphonenumber-for-php": "^8.10",
        "jackalope/jackalope-doctrine-dbal": "*",
        "moneyphp/money": "^3.2",
        "myclabs/php-enum": "^1.6",
        "phpunit/phpunit": "^8.0",
        "symfony/cache": "^4.2|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Refugis\\DoctrineExtra\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Refugis\\DoctrineExtra\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "doctrine/orm": ">=2.8",
        "doctrine/dbal": ">=2.11"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "ext-mongo": "1.6.16",
            "ext-mongodb": "1.3.0"
        }
    }
}
